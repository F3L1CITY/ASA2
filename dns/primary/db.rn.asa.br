@	IN	NS	ns1.rn.asa.br.
@	IN	MX	10 mail.rn.asa.br.
@	IN	A	10.25.3.135

ns1	IN	A	10.25.3.135
mail	IN	A	10.25.3.135
www	IN	CNAME	ns1
